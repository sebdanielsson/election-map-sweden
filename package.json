    "dotenv": "^16.4.5",
    "mapbox-gl": "^3.7.0",
    "next": "15.0.4",
    "proj4": "^2.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/geojson": "7946.0.15",
    "@types/node": "^22.0.0",
    "@types/proj4": "^2.5.5",
