{
  "name": "election-map-swe",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env.development -- next dev --turbopack",
    "build": "dotenvx run -f .env.production -- next build",
    "start": "dotenvx run -f .env.production -- next start",
    "lint": "eslint .",
    "dotenvx": "dotenvx",
    "download-districts": "bun --bun run scripts/downloadDistricts.ts temp/districts/EPSG3006",
    "transform-geojson": "bun --bun run scripts/transformGeojson.ts temp/districts/EPSG3006 public/data/districts",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "stylelint": "stylelint --allow-empty-input \"app/**/*.css\""
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.4",
    "@types/unzipper": "^0.10.11",
    "@vercel/speed-insights": "^1.3.1",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "mapbox-gl": "^3.18.0",
    "next": "16.1.4",
    "proj4": "^2.20.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@dreamsicle.io/stylelint-config-tailwindcss": "^1.1.2",
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "16.1.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/geojson": "7946.0.16",
    "@types/node": "^25.0.9",
    "@types/proj4": "^2.19.0",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/webpack-env": "^1.18.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.4",
    "oxfmt": "^0.27.0",
    "postcss": "^8.5.6",
    "stylelint": "^17.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^40.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "unzipper": "^0.12.3"
  },
  "overrides": {
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "engines": {
    "bun": ">=1.3.6"
  }
}
